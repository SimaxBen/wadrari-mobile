{
  "expo": {
  "name": "wadrari",
  "slug": "wadrari-mobile",
      "version": "1",
    "orientation": "portrait",
    "icon": "./assets/5927092260921526942.jpg",
    "userInterfaceStyle": "automatic", 
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#1a1a2e"
    },
